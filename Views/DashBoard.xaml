<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NoteApp.Views.DashBoard"
             xmlns:tabbar="clr-namespace:NoteApp.Views.components.Navbar"
             xmlns:dashboard="clr-namespace:NoteApp.Views.components.Dashboard"
             Title="DashBoard">

    <ScrollView>
        <VerticalStackLayout Padding="10" >
            <dashboard:NoteVoiceDashboard></dashboard:NoteVoiceDashboard>
            <dashboard:NoteImgDashboard></dashboard:NoteImgDashboard>
            <dashboard:NoteTextDashboard></dashboard:NoteTextDashboard>
            <dashboard:ReminderDashboard></dashboard:ReminderDashboard>
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>